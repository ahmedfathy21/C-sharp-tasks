using System;
using System.Globalization;
namespace Assignment_2{
    class Program{
        public static void Main(string[] args){
                #region Assignment_2 >> Assigning one value type variable to another and modifying the value of one variable and mention what will happen
            int First_Number = 10;
            First_Number = 123456;
            double Second_Number = First_Number;
            Console.WriteLine("First Number : " + First_Number);
            Console.WriteLine("Second Number : " + Second_Number);
            Console.WriteLine("this is now a implicit Casting");
            Second_Number = 123.456;
            int Explicit_num = (int)Second_Number;
            Console.WriteLine("this is now a Explicit Casting");
            Console.WriteLine("Excplict_Number "+ Explicit_num);


                #endregion
    }

}}  